<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Accom_Seeker" android:visibility="visible">

    <com.google.android.material.textfield.TextInputEditText
            android:layout_width="340dp"
            android:layout_height="53dp"
            android:hint="Indentifiction Number" android:id="@+id/Seek_Iden" android:layout_weight="1"
            android:layout_marginTop="35dp"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="63dp" android:layout_marginStart="63dp"/>
    <Button
            android:text="Location"
            android:layout_width="326dp"
            android:layout_height="47dp"
            android:id="@+id/Seek_loc" android:layout_marginTop="76dp"
            app:layout_constraintTop_toBottomOf="@+id/Seek_Iden" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="55dp" android:layout_marginStart="55dp"/>
    <com.google.android.material.textfield.TextInputLayout
            android:layout_width="337dp"
            android:layout_height="69dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="104dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="66dp" android:layout_marginStart="66dp" android:id="@+id/textInputLayout2">

        <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="53dp"
                android:hint="Name" android:id="@+id/Seek_Name" android:visibility="visible"/>
    </com.google.android.material.textfield.TextInputLayout>
    <com.google.android.material.textfield.TextInputLayout
            android:layout_width="395dp"
            android:layout_height="wrap_content"
            android:id="@+id/textInputLayout" android:layout_marginTop="22dp"
            app:layout_constraintTop_toBottomOf="@+id/textInputLayout2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="64dp" android:layout_marginStart="64dp">
        <com.google.android.material.textfield.TextInputEditText
                android:layout_width="340dp"
                android:layout_height="53dp"
                android:hint="Mobile" android:id="@+id/Seek_Mob" android:layout_weight="1"
                android:visibility="visible"/>
    </com.google.android.material.textfield.TextInputLayout>
    <Button
            android:text="Register"
            android:layout_width="315dp"
            android:layout_height="52dp"
            android:id="@+id/Seek_Reg" android:layout_marginTop="47dp"
            app:layout_constraintTop_toBottomOf="@+id/Seek_loc" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="63dp" android:layout_marginStart="63dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>